@model Int32

@{
    var range = Enumerable.Range(0, @Model+1)
        .Reverse()
        .Select(i => new SelectListItem(){ Text = i.ToString(), Value = i.ToString() });
}

@Html.DropDownList("", range, new  { @class = "mobiscroll f-dd" , @style = "width:50px;", @data_role="none", @mode="mixed", @width=100 } )
